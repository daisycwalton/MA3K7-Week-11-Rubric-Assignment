   "source": [
    "import numpy as np\n",
    "from numpy import random"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "id": "cdee64e5-0d91-462b-ac81-84e9bc9e16fa",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "3 5 8\n",
      "6 1 7\n",
      "9 2 4\n"
     ]
    }
   ],
   "source": [
    "numbers = np.arange(1,10)\n",
    "random.shuffle(numbers)\n",
    "grid = [numbers[i:i+3] for i in range(0, 9, 3)]\n",
    "for row in grid:\n",
    "    print(' '.join(map(str, row)))"
   ]
